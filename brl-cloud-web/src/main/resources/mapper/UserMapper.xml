<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.brl.dao.UserDao" >

    <select id="findByUserNameXmlSql" parameterType="java.lang.String" resultType="com.brl.model.User">
        select * from brl_user where user_name = #{userName}
    </select>

</mapper>

